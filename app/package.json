{
  "private": true,
  "version": "0.3.0-beta1",
  "name": "SturdyDev",
  "main": "dist/main.js",
  "scripts": {
    "dev": "rollup -c rollup.config.js && yarn electron-dev",
    "dev-prod": "rollup -c rollup.config.js && yarn electron-dev-prod",
    "electron-dev": "cross-env AGENT_DIR_NAME=sturdy-agent-dev MUTAGEN_DIR_NAME=mutagen-dev STURDY_OLD_CONFIG_PATH=$HOME/.sturdy-local STURDY_POSTHOG_API_KEY=AFCRFwnQF8Y_Y4yvPasmyf3UOxG3rJVEGC6v4NG_ZRo STURDY_DEFAULT_BACKEND=development electron .",
    "electron-dev-prod": "cross-env AGENT_DIR_NAME=sturdy-agent-dev-prod MUTAGEN_DIR_NAME=mutagen-dev-prod STURDY_OLD_CONFIG_PATH=$HOME/.sturdy STURDY_POSTHOG_API_KEY=AFCRFwnQF8Y_Y4yvPasmyf3UOxG3rJVEGC6v4NG_ZRo electron .",
    "build": "rollup -c",
    "format": "prettier --write .",
    "pack": "electron-builder --dir",
    "dev2-mac": "rollup -c && electron-builder --mac --x64 --dir -c.mac.identity=null && AGENT_DIR_NAME=sturdy-agent-dev-prod-2 MUTAGEN_DIR_NAME=mutagen-dev-prod-2 STURDY_OLD_CONFIG_PATH=$HOME/.sturdy-old-something STURDY_POSTHOG_API_KEY=AFCRFwnQF8Y_Y4yvPasmyf3UOxG3rJVEGC6v4NG_ZRo STURDY_DISABLE_AUTO_UPDATER=1 ./dist/mac/Sturdy.app/Contents/MacOS/Sturdy",
    "dev-mac-x64-prodlike": "rollup -c && electron-builder --mac --x64 --dir -c.mac.identity=null && cross-env STURDY_DISABLE_AUTO_UPDATER=1 ./dist/mac/Sturdy.app/Contents/MacOS/Sturdy"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "cross-env": "^7.0.3",
    "electron": "^16.0.0",
    "electron-builder": "^22.14.5",
    "electron-notarize": "^1.1.1",
    "prettier": "^2.5.0",
    "rollup": "^2.59.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@sentry/electron": "^2.5.4",
    "@sentry/integrations": "^6.15.0",
    "@urql/core": "^2.3.4",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.6.1",
    "graphql": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^3.1.0",
    "posthog-node": "^1.3.0",
    "sshpk": "^1.16.1",
    "tslib": "^2.3.1"
  }
}
